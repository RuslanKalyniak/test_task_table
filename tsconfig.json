  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/index.js", "pages/_app.js", "src/component/Header/Header.js", "src/component/Layout/Layout.js", "src/component/Layout/Layout.js", "src/component/Theme/index.js", "src/StaticData/makeId.js", "src/StaticData/ABOUT_ME.js"],
  "exclude": ["node_modules"]
}
